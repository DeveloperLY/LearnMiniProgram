<!--pages/home/child-cpns/ly-recommend/ly-recommend.wxml-->
<view class='recommend'>
  <block wx:for='{{recommends}}' wx:key='{{index}}'>
    <view class='recommend-item'>
      <image src='{{item.image}}' bindload='handleImageLoad'/>
      <view>{{item.title}}</view>
    </view>
  </block>
</view>
